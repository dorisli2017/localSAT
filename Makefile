all:
	g++ -O3 ln.cpp -std=c++11 -o ln
	g++ -O3 lnt.cpp -std=c++11 -o lnt
	g++ -O3 lnd.cpp -std=c++11 -o lnd
	g++ -O3 ls.cpp -std=c++11 -o ls
	g++ -O3 lsd.cpp -std=c++11 -o lsd
	g++ -O3 lst.cpp -std=c++11 -o lst
	g++ -O3 lw.cpp -std=c++11 -o lw
	g++ -O3 lwd.cpp -std=c++11 -o lwd
	g++ -O3 lwt.cpp -std=c++11 -o lwt
clean:
	rm l
